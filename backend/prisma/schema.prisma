generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WORKERS {
  WORKER_ID    Int     @id
  WORKER_NAME  String?
  PASSWORD     String?
  ROLE_ID      Int?
  DEPARTMENT   String?
  DIVISION     String?
  DOMAIN       String?
  TEAM         String?
  ACTIVE       Boolean @default(true)
}